
{
  "buildCommand": "npm install --legacy-peer-deps && npm install vite@latest -g && npx vite build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "routes": [
    {
      "src": "/api/og",
      "dest": "/api/og.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
